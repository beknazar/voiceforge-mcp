[24.326000000000068,"o","  #### `agent.py`\r\n"]
[24.33800000000007,"o","  ```python\r\n"]
[24.35000000000007,"o","  \"\"\"\r\n"]
[24.36200000000007,"o","  thai-support-agent - Voice AI Agent\r\n"]
[24.37400000000007,"o","  Generated by VoiceForge MCP\r\n"]
[24.38600000000007,"o","  \r\n"]
[24.39800000000007,"o","  Stack: Deepgram nova-3 → Google gemini-2.5-flash → Cartesia sonic-3\r\n"]
[24.41000000000007,"o","  Language: Thai | Use Case: customer support\r\n"]
[24.42200000000007,"o","  \"\"\"\r\n"]
[24.434000000000072,"o","  \r\n"]
[24.446000000000073,"o","  import logging\r\n"]
[24.458000000000073,"o","  from dotenv import load_dotenv\r\n"]
[24.470000000000073,"o","  from livekit.agents import AutoSubscribe, JobContext, WorkerOptions, cli, llm\r\n"]
[24.482000000000074,"o","  from livekit.agents.pipeline import VoicePipelineAgent\r\n"]
[24.494000000000074,"o","  from livekit.plugins import deepgram\r\n"]
[24.506000000000075,"o","  from livekit.plugins import google\r\n"]
[24.518000000000075,"o","  from livekit.plugins import cartesia\r\n"]
[24.530000000000076,"o","  \r\n"]
[24.542000000000076,"o","  load_dotenv()\r\n"]
[24.554000000000077,"o","  logger = logging.getLogger(\"thai-support-agent\")\r\n"]
[24.566000000000077,"o","  \r\n"]
[24.578000000000078,"o","  \r\n"]
[24.590000000000078,"o","  async def entrypoint(ctx: JobContext):\r\n"]
[24.60200000000008,"o","      await ctx.connect(auto_subscribe=AutoSubscribe.AUDIO_ONLY)\r\n"]
[24.61400000000008,"o","  \r\n"]
[24.62600000000008,"o","      initial_ctx = llm.ChatContext().append(\r\n"]
[24.63800000000008,"o","          role=\"system\",\r\n"]
[24.65000000000008,"o","          text=(\r\n"]
[24.66200000000008,"o","              \"You are a voice AI agent for customer support. \"\r\n"]
[24.67400000000008,"o","              \"You communicate in Thai. \"\r\n"]
[24.68600000000008,"o","              \"Keep responses concise (1-2 sentences). \"\r\n"]
[24.698000000000082,"o","              \"Be natural, warm, and helpful.\"\r\n"]
[24.710000000000083,"o","          ),\r\n"]
[24.722000000000083,"o","      )\r\n"]
[24.734000000000083,"o","  \r\n"]
[24.746000000000084,"o","      agent = VoicePipelineAgent(\r\n"]
[24.758000000000084,"o","          vad=ctx.proc.userdata.get(\"vad\"),\r\n"]
[24.770000000000085,"o","          stt=deepgram.STT(model=\"nova-3\"),\r\n"]
[24.782000000000085,"o","          llm=google.LLM(model=\"gemini-2.5-flash\"),\r\n"]
[24.794000000000086,"o","          tts=cartesia.TTS(model=\"sonic-3\"),\r\n"]
[24.806000000000086,"o","          chat_ctx=initial_ctx,\r\n"]
[24.818000000000087,"o","      )\r\n"]
[24.830000000000087,"o","  \r\n"]
[24.842000000000088,"o","      agent.start(ctx.room)\r\n"]
[24.854000000000088,"o","      await agent.say(\r\n"]
[24.86600000000009,"o","          \"Hello! How can I help you today?\",\r\n"]
[24.87800000000009,"o","          allow_interruptions=True,\r\n"]
[24.89000000000009,"o","      )\r\n"]
[24.90200000000009,"o","  \r\n"]
[24.91400000000009,"o","  \r\n"]
[24.92600000000009,"o","  if __name__ == \"__main__\":\r\n"]
[24.93800000000009,"o","      cli.run_app(WorkerOptions(entrypoint_fnc=entrypoint))\r\n"]
[24.95000000000009,"o","  \r\n"]
[24.962000000000092,"o","  ```\r\n"]
